{
  "name": "vue-mac-keyboard",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.9.0",
  "description": "Macbook computer keyboard style for VueJS component.",
  "keywords": [
    "vue",
    "keyboard",
    "macbook"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com",
    "url": "https://ntnyq.com"
  },
  "repository": "ntnyq/vue-mac-keyboard",
  "exports": {
    "./package.json": "./package.json",
    "./style": "./dist/style.css",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:style": "sass src/style.scss dist/style.css --no-source-map --style=compressed",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "pnpm -C playground run dev",
    "play:build": "pnpm -C playground run build",
    "prepare": "husky",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^2.8.2",
    "@ntnyq/prettier-config": "^1.21.2",
    "@types/node": "^20.16.3",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vitest/coverage-v8": "^2.0.5",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "happy-dom": "^15.7.3",
    "husky": "^9.1.5",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,md,yml,yaml,json}": "eslint --fix"
  }
}
